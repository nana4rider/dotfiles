[
  // グローバルキーバインド設定ファイルを開く
  {
    "key": "ctrl+shift+,",
    "command": "workbench.action.openGlobalKeybindingsFile"
  },
  // 設定ファイル (settings.json) を開く
  {
    "key": "ctrl+oem_comma",
    "command": "workbench.action.openSettingsJson"
  },
  // 新しい空のファイルを作成
  {
    "key": "ctrl+t",
    "command": "workbench.action.files.newUntitledFile"
  },
  // 置換
  {
    "key": "ctrl+shift+f",
    "command": "editor.action.startFindReplaceAction",
    "when": "editorFocus || editorIsOpen"
  },
  // シンボル検索を開く
  {
    "key": "ctrl+n",
    "command": "workbench.action.showAllSymbols"
  },
  // 文字列変換
  {
    "key": "ctrl+k ctrl+k",
    "command": "extension.changeCase.pascal",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+k ctrl+s",
    "command": "extension.changeCase.snake",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+k ctrl+u",
    "command": "editor.action.transformToUppercase"
  },
  {
    "key": "ctrl+k ctrl+l",
    "command": "editor.action.transformToLowercase"
  },
  // カーソル行を1行下に移動
  {
    "key": "ctrl+down",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  // カーソル行を1行上に移動
  {
    "key": "ctrl+up",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  // 検索ダイアログを開く
  {
    "key": "alt+oem_2",
    "command": "workbench.action.findInFiles"
  },
  // 置換ダイアログを開く
  {
    "key": "shift+alt+oem_2",
    "command": "workbench.action.replaceInFiles"
  },
  // すべて置換
  {
    "key": "alt+a",
    "command": "editor.action.replaceAll",
    "when": "editorFocus && findWidgetVisible"
  },
  {
    "key": "alt+a",
    "command": "search.action.replaceAll",
    "when": "replaceActive && searchViewletVisible && !findWidgetVisible"
  },
  // 現在の行の後ろに改行を挿入
  {
    "key": "shift+enter",
    "command": "editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
  },
  // 次の一致を選択に追加
  {
    "key": "alt+j",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  // カーソル操作を元に戻す
  {
    "key": "alt+k",
    "command": "cursorUndo",
    "when": "textInputFocus"
  },
  // 次のタブに移動
  {
    "key": "ctrl+tab",
    "command": "workbench.action.nextEditor"
  },
  // 前のタブに移動
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.previousEditor"
  },
  // 正規表現モードを切り替え
  {
    "key": "alt+g",
    "command": "toggleFindRegex",
    "when": "editorFocus"
  },
  {
    "key": "alt+g",
    "command": "workbench.action.terminal.toggleFindRegex",
    "when": "terminalFindFocused && terminalProcessSupported || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+g",
    "command": "toggleSearchEditorRegex",
    "when": "inSearchEditor && searchInputBoxFocus"
  },
  {
    "key": "alt+g",
    "command": "toggleSearchRegex",
    "when": "searchViewletFocus"
  },
  // Diffエディタで次の変更箇所に移動
  {
    "key": "alt+down",
    "command": "workbench.action.compareEditor.nextChange",
    "when": "textCompareEditorVisible"
  },
  // Diffエディタで前の変更箇所に移動
  {
    "key": "alt+up",
    "command": "workbench.action.compareEditor.previousChange",
    "when": "textCompareEditorVisible"
  },
  // エディタグループにフォーカス
  {
    "key": "f19",
    "command": "workbench.action.focusFirstEditorGroup"
  },
  // エクスプローラを開く
  {
    "key": "f18",
    "command": "workbench.view.explorer",
    "when": "viewContainer.workbench.view.explorer.enabled"
  },
  {
    "key": "f18",
    "command": "workbench.action.quickOpenNavigatePreviousInFilePicker",
    "when": "inFilesPicker && inQuickOpen"
  },
  // ターミナルをトグル
  {
    "key": "f13",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  // 新しいターミナルを作成
  {
    "key": "ctrl+=",
    "command": "workbench.action.terminal.new",
    "when": "terminalProcessSupported"
  },
  // シンボルに移動
  {
    "key": "ctrl+o",
    "command": "workbench.action.gotoSymbol"
  },
  // ファイルを開く
  {
    "key": "ctrl+shift+o",
    "command": "workbench.action.files.openFile",
    "when": "true"
  },
  {
    "key": "ctrl+shift+o",
    "command": "workbench.action.files.openLocalFile",
    "when": "remoteFileDialogVisible"
  },
  // ファイルを閉じる
  {
    "key": "alt+q",
    "command": "workbench.action.closeActiveEditor"
  },
  // 参照を開く
  {
    "key": "ctrl+alt+h",
    "command": "editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  // 定義を開く
  {
    "key": "alt+b",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  // スマート選択 (範囲を拡大)
  {
    "key": "alt+down",
    "command": "editor.action.smartSelect.expand",
    "when": "editorTextFocus && !textCompareEditorVisible"
  },
  // スマート選択 (範囲を縮小)
  {
    "key": "alt+up",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus && !textCompareEditorVisible"
  },
  // 行の削除
  {
    "key": "ctrl+d",
    "command": "editor.action.deleteLines"
  }
  // デフォルトキーバインドの削除
  {
    "key": "ctrl+k ctrl+s",
    "command": "-workbench.action.openGlobalKeybindings"
  },
  {
    "key": "ctrl+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+t",
    "command": "-workbench.action.showAllSymbols"
  },
  {
    "key": "alt+down",
    "command": "-editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+up",
    "command": "-editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+h",
    "command": "-workbench.action.replaceInFiles"
  },
  {
    "key": "ctrl+alt+enter",
    "command": "-editor.action.replaceAll",
    "when": "editorFocus && findWidgetVisible"
  },
  {
    "key": "ctrl+alt+enter",
    "command": "-search.action.replaceAll",
    "when": "replaceActive && searchViewletVisible && !findWidgetVisible"
  },
  {
    "key": "ctrl+k ctrl+c",
    "command": "-editor.action.addCommentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+k ctrl+u",
    "command": "-editor.action.removeCommentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+d",
    "command": "-editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+shift+k",
    "command": "-editor.action.deleteLines",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+pagedown",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "ctrl+pageup",
    "command": "-workbench.action.previousEditor"
  },
  {
    "key": "alt+r",
    "command": "-workbench.action.terminal.toggleFindRegex",
    "when": "terminalFindFocused && terminalProcessSupported || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+r",
    "command": "-toggleFindRegex",
    "when": "editorFocus"
  },
  {
    "key": "alt+r",
    "command": "-toggleSearchEditorRegex",
    "when": "inSearchEditor && searchInputBoxFocus"
  },
  {
    "key": "alt+r",
    "command": "-toggleSearchRegex",
    "when": "searchViewletFocus"
  },
  {
    "key": "ctrl+f2",
    "command": "-editor.action.changeAll",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+f5",
    "command": "-workbench.action.compareEditor.nextChange",
    "when": "textCompareEditorVisible"
  },
  {
    "key": "shift+alt+f5",
    "command": "-workbench.action.compareEditor.previousChange",
    "when": "textCompareEditorVisible"
  },
  {
    "key": "ctrl+shift+f",
    "command": "-workbench.action.findInFiles"
  },
  {
    "key": "ctrl+shift+m",
    "command": "-workbench.actions.view.problems",
    "when": "workbench.panel.markers.view.active"
  },
  {
    "key": "ctrl+shift+u",
    "command": "-workbench.action.output.toggleOutput",
    "when": "workbench.panel.output.active"
  },
  {
    "key": "ctrl+oem_3",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+shift+oem_3",
    "command": "-workbench.action.terminal.new",
    "when": "terminalProcessSupported"
  },
  {
    "key": "ctrl+shift+o",
    "command": "-workbench.action.gotoSymbol"
  },
  {
    "key": "ctrl+o",
    "command": "-workbench.action.files.openFile",
    "when": "true"
  },
  {
    "key": "ctrl+o",
    "command": "-workbench.action.files.openLocalFile",
    "when": "remoteFileDialogVisible"
  },
  {
    "key": "shift+f12",
    "command": "-editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "ctrl+k f12",
    "command": "-editor.action.revealDefinitionAside",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "shift+alt+left",
    "command": "-editor.action.smartSelect.shrink",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+right",
    "command": "-editor.action.smartSelect.expand",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+alt+left",
    "command": "-cursorColumnSelectLeft",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+down",
    "command": "-cursorColumnSelectDown",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+pagedown",
    "command": "-cursorColumnSelectPageDown",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+pageup",
    "command": "-cursorColumnSelectPageUp",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+up",
    "command": "-cursorColumnSelectUp",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+alt+right",
    "command": "-cursorColumnSelectRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+j",
    "command": "-workbench.action.togglePanel"
  },
  {
    "key": "ctrl+oem_comma",
    "command": "-workbench.action.openSettings"
  },
  {
    "key": "ctrl+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+k ctrl+l",
    "command": "-editor.toggleFold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "alt+space",
    "command": "-workbench.action.terminal.sendSequence",
    "when": "terminalFocus && terminalShellIntegrationEnabled && !accessibilityModeEnabled && terminalShellType == 'pwsh'"
  },
  {
    "key": "ctrl+d",
    "command": "-notebook.addFindMatchToSelection",
    "when": "config.notebook.multiCursor.enabled && notebookCellEditorFocused && activeEditor == 'workbench.editor.notebook'"
  }
]
