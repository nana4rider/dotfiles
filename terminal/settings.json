    "actions": 
    [
        {
            "command": "paste",
            "keys": "alt+v"
        },
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": 
            {
                "action": "nextTab",
                "tabSwitcherMode": "disabled"
            },
            "keys": "ctrl+tab"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "closeTab"
            },
            "keys": "alt+q"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": 
            {
                "action": "prevTab",
                "tabSwitcherMode": "disabled"
            },
            "keys": "ctrl+shift+tab"
        },
        {
            "command": "openNewTabDropdown",
            "keys": "alt+f"
        },
        {
            "command": 
            {
                "action": "openSettings",
                "target": "settingsFile"
            },
            "keys": "ctrl+comma"
        },
        {
            "command": 
            {
                "action": "openSettings",
                "target": "settingsUI"
            },
            "keys": "ctrl+shift+comma"
        }
    ],
